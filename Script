local function CWATOHI_fake_script() -- Airstuck.LocalScript 
	local script = Instance.new('LocalScript', Airstuck)

	script.Parent.MouseButton1Click:Connect(function()
		-- Variables
	
		local player = game.Players.LocalPlayer
		local mouse = player:GetMouse()
	
		-- Settings
	
		bind = "e" -- has to be lowercase
		bind2 = "r"
	
		-- Script
	
		mouse.KeyDown:connect(function(key)
			if key == bind then
				print("Currently Airstuck")
				game.Players.LocalPlayer.Character.Torso.Anchored = true
			end
		end)
	
		mouse.KeyDown:connect(function(key2)
			if key2 == bind2 then
				print("Currently Unstuck!")
				game.Players.LocalPlayer.Character.Torso.Anchored = false
			end
		end)
	end)
	
	
	
	
	
end
coroutine.wrap(CWATOHI_fake_script)()
